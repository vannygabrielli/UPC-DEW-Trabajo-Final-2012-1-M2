<h1>Listing administradors</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Correo</th>
    <th>Estado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @administradors.each do |administrador| %>
  <tr>
    <td><%= administrador.nombre %></td>
    <td><%= administrador.apellido %></td>
    <td><%= administrador.correo %></td>
    <td><%= administrador.Estado %></td>
    <td><%= link_to 'Show', administrador %></td>
    <td><%= link_to 'Edit', edit_administrador_path(administrador) %></td>
    <td><%= link_to 'Destroy', administrador, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

