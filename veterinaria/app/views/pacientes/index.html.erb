<h1>Listing pacientes</h1>

<table>
  <tr>
    <th>Especie</th>
    <th>Raza</th>
    <th>Genero</th>
    <th>Tipo sangre</th>
    <th>Esterilizado</th>
    <th>Tamanio</th>
    <th>Actividad</th>
    <th>Peso</th>
    <th>Fec nacimiento</th>
    <th>Cliente</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pacientes.each do |paciente| %>
  <tr>
    <td><%= paciente.especie %></td>
    <td><%= paciente.raza %></td>
    <td><%= paciente.genero %></td>
    <td><%= paciente.tipo_sangre %></td>
    <td><%= paciente.esterilizado %></td>
    <td><%= paciente.tamanio %></td>
    <td><%= paciente.actividad %></td>
    <td><%= paciente.peso %></td>
    <td><%= paciente.fec_nacimiento %></td>
    <td><%= paciente.Cliente %></td>
    <td><%= link_to 'Show', paciente %></td>
    <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
    <td><%= link_to 'Destroy', paciente, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Paciente', new_paciente_path %>
