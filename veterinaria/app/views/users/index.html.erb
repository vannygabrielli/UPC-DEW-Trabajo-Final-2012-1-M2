<h1>Listing users</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Correo</th>
    <th>Housephone</th>
    <th>Mobilephone</th>
    <th>Address</th>
    <th>Photo</th>
    <th>Profile</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.housephone %></td>
    <td><%= user.mobilephone %></td>
    <td><%= user.address %></td>
    <td><%= user.photo %></td>
    <td><%= user.profile.name %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
